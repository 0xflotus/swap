{
    "name": "florianv/swap",
    "type": "library",
    "description": "Exchange rates library for PHP 5.3+",
    "keywords": ["currency", "money", "rate", "conversion", "exchange rates"],
    "homepage": "https://github.com/florianv/swap",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://florian.voutzinos.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Swap": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Swap\\Tests": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "guzzle/guzzle": "~3.0"
    },
    "suggest": {
        "guzzle/guzzle": "If you want to use the Guzzle3Adapter",
        "guzzlehttp/guzzle": "If you want to use the Guzzle4Adapter"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
